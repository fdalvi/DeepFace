# Head 1
layer {
  bottom: "fc7"
  top: "fc8-1"
  name: "fc8-1"
  type: "InnerProduct"
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  bottom: "fc8-1"
  bottom: "label-1"
  top: "loss-1"
  name: "loss-1"
  type: "SoftmaxWithLoss"
}
