# DeepFace Network
name: "DeepFace_layers"
# input: "data"
# input_dim: 3
# input_dim: 3
# input_dim: 224
# input_dim: 224
# input: "label"
layer {
  top: "label"
  name: "label"
  type: "Data"
  data_param {
    source: "../data/labels-lmdb/"
    backend: LMDB
    batch_size: 3
  }
  include: {
    phase: TRAIN
  }
}
layer {
  top: "label"
  name: "label"
  type: "Data"
  data_param {
    source: "../data/labels-lmdb-test/"
    backend: LMDB
    batch_size: 3
  }
  include: {
    phase: TEST
  }
}
layer {
  name: "slicer_label"
  type: "Slice"
  bottom: "label"
  top: "label-1"
  top: "label-2"
  slice_param {
    slice_dim: 1
    slice_point: 1
  }
}
layer {
  top: "data"
  name: "data"
  type: "Data"
  data_param {
    source: "../data/dev_set/dataset.lmdb/"
    backend: LMDB
    batch_size: 3
  }
  include: {
    phase: TRAIN
  }
}
layer {
  top: "data"
  name: "data"
  type: "Data"
  data_param {
    source: "../data/dev_set/dataset-test.lmdb/"
    backend: LMDB
    batch_size: 3
  }
  include: {
    phase: TEST
  }
}
layer {
  bottom: "data"
  top: "conv1_1"
  name: "conv1_1"
  type: "Convolution"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
}
layer {
  bottom: "conv1_1"
  top: "conv1_1"
  name: "relu1_1"
  type: "ReLU"
}
layer {
  bottom: "conv1_1"
  top: "conv1_2"
  name: "conv1_2"
  type: "Convolution"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
}
layer {
  bottom: "conv1_2"
  top: "conv1_2"
  name: "relu1_2"
  type: "ReLU"
}
layer {
  bottom: "conv1_2"
  top: "pool1"
  name: "pool1"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}
layer {
  bottom: "pool1"
  top: "conv2_1"
  name: "conv2_1"
  type: "Convolution"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 3
  }
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
}
layer {
  bottom: "conv2_1"
  top: "conv2_1"
  name: "relu2_1"
  type: "ReLU"
}
layer {
  bottom: "conv2_1"
  top: "conv2_2"
  name: "conv2_2"
  type: "Convolution"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 3
  }
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
}
layer {
  bottom: "conv2_2"
  top: "conv2_2"
  name: "relu2_2"
  type: "ReLU"
}
layer {
  bottom: "conv2_2"
  top: "pool2"
  name: "pool2"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}
layer {
  bottom: "pool2"
  top: "conv3_1"
  name: "conv3_1"
  type: "Convolution"
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 3
  }
  param {
