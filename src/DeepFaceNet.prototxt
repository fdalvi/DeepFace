  bottom: "fc7"
  top: "fc8-2"
  name: "fc8-2"
  type: "InnerProduct"
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
 bottom: "fc8-2"
 bottom: "label-2"
 top: "loss-2"
 name: "loss-2"
 type: "SoftmaxWithLoss"
}
